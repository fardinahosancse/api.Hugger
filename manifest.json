{
  "manifest_version": 3,
  "name": "api.Hugger",
  "version": "1.0",
  "description": "A modern API Manager for storing and organizing your API keys",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.svg",
      "48": "images/icon48.svg",
      "128": "images/icon128.svg"
    }
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "images/icon16.svg",
    "48": "images/icon48.svg",
    "128": "images/icon128.svg"
  }
}